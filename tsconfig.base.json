{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "ES2022",
    "lib": [
      "ES2023",
    ],
    "module": "CommonJS",
    "allowJs": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "composite": true,
    "declaration": true,
    "strict": true,
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strictNullChecks": true,
    "moduleResolution":"node",
  },
  "exclude": ["out", "dist"]
}