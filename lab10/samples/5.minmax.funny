min(x: int, y: int) 
    returns m: int
    ensures m <= x and m <= y and (m == x or m == y)
{
    if(x < y)
        m = x;
    else
        m = y;
}

max(x: int, y: int) 
    returns m: int
    ensures m >= x and m >= y and (m == x or m == y)
{
    if(x > y)
        m = x;
    else
        m = y;
}
